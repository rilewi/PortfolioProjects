_____________________________________________________________________________________________________________________________________________________#\n","\n","# Isolate the salaries of large cities and metropolises\n","citysal = df.loc[df['PopulationOfLargestCityWithin20Miles'] == '300K-1M (large city)', 'SalaryUSD'].dropna()\n","metrosal = df.loc[df['PopulationOfLargestCityWithin20Miles'] == '1M+ (metropolis)', 'SalaryUSD'].dropna()\n","\n","\n","# Create the barplot\n","sns.barplot(x=['City', 'Metro'], y=[citysal.mean(), metrosal.mean()], errorbar=\"ci\", capsize=0.2)\n","\n","# Add the actual salary value to each bar\n","bars = sns.barplot(x=['City', 'Metro'], y=[citysal.mean(), metrosal.mean()], errorbar=\"ci\", capsize=0.2)\n","for bar, salary in zip(bars.patches, [citysal.mean(), metrosal.mean()]):\n","    # Get the y-coordinate of the bar\n","    y = bar.get_height()\n","    # Add the salary value as text on top of the bar\n","    bars.text(bar.get_x() + bar.get_width()/2, y, f'{salary:.2f}', ha='center', va='bottom')\n","\n","# Set the y-axis label\n","plt.ylabel('Salary USD')\n","\n","# Set the y-axis limits\n","plt.ylim(96000, 106000)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":452},"id":"FjRuHcX5FFUP","executionInfo":{"status":"ok","timestamp":1682650449998,"user_tz":420,"elapsed":492,"user":{"displayName":"Riley Lewis","userId":"03987083361925044165"}},"outputId":"537860f6-28d5-438b-a8de-44a7a0f49f4f"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["(96000.0, 106000.0)"]},"metadata":{},"execution_count":13},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["#Show the data types we are working with\n","print(df[keep[2:4]].dtypes)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"P6s0OjVjxs3z","executionInfo":{"status":"ok","timestamp":1682650398273,"user_tz":420,"elaps